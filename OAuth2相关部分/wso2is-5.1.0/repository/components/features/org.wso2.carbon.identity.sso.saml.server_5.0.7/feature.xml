<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature id="org.wso2.carbon.identity.sso.saml.server" label="Identity SAML Single Sign-on Server Feature" provider-name="%providerName" version="5.0.7">
  <description>This feature contains the core bundles required for Back-end SAML2.0 based Single
        Sign-on functionality</description>
  <copyright>%copyright</copyright>
  <license url="%licenseURL">%license</license>
  <plugin id="opensaml2" unpack="false" version="2.4.1.wso2v1"/>
  <plugin id="org.wso2.carbon.identity.sso.saml" unpack="false" version="5.0.7"/>
  <require>
    <import feature="org.wso2.carbon.identity.authenticator.saml2.sso.server" match="equivalent" version="5.0.7"/>
    <import feature="org.wso2.carbon.identity.application.authentication.framework.server" match="equivalent" version="5.0.7"/>
    <import feature="org.wso2.carbon.security.mgt.server" match="equivalent" version="5.0.7"/>
    <import feature="org.wso2.carbon.identity.application.mgt.server" match="equivalent" version="5.0.7"/>
    <import feature="org.wso2.carbon.identity.core" match="equivalent" version="5.0.7"/>
    <import feature="org.wso2.carbon.core" match="equivalent" version="4.4.3"/>
    <import feature="org.wso2.carbon.idp.mgt.server" match="equivalent" version="5.0.7"/>
  </require>
</feature>
